<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUIApp5.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUIApp5.Pages"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem
        FlyoutDisplayOptions="AsMultipleItems">
        
         <ShellContent
            Title="Taylor Swift"
            ContentTemplate="{DataTemplate local:TaylorSwift}"
            Route="TaylorSwift" 
             FlyoutIcon="Taylor.jpg"/>
        

        <ShellContent
            Title="Nicole Zefanya"
            ContentTemplate="{DataTemplate local:Niki}"
            Route="Niki" 
            FlyoutIcon="Niki.jpg"/>

        <ShellContent
            Title="SabrinaCarpenter"
            ContentTemplate="{DataTemplate local:SabrinaCarpenter}"
            Route="SabrinaCarpenter" 
            FlyoutIcon="Sabrina.jpg"/>
    </FlyoutItem>

    

</Shell>
